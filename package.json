{
  "name": "pubpub",
  "version": "3.0.0",
  "description": "PubPub - Open Publishing",
  "main": "exports.js",
  "author": "PubPub <pubpub@media.mit.edu>",
  "license": "GPL-2.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/pubpub/pubpub.git"
  },
  "scripts": {
    "build": "env UV_THREADPOOL_SIZE=100; NODE_ENV=production webpack --config ./webpack.production.config.js",
    "profile": "NODE_ENV=production webpack --config ./webpack.production.config.js --profile --json  > stats.json",
    "build-dev": "npm install; npm run build; mv _redirects-dev dist/_redirects",
    "build-prod": "npm install; npm run build; mv _redirects-prod dist/_redirects",
    "lint": "eslint app",
    "start": "NODE_PATH=./app node server --preserve-symlinks",
    "start-prod": "NODE_PATH=./app NODE_ENV=production node server",
    "start-prod-local": "npm run build && NODE_PATH=./app NODE_ENV=production PORT=3000 node server",
    "test": "NODE_PATH=./app TESTING=true mocha $(find app -name '*.test.js') --compilers js:babel-register --require ./app/tests/mochaEnvInit.js --watch --watch-extensions js --reporter list",
    "storybook": "start-storybook -p 9001",
    "build-storybook": "build-storybook"
  },
